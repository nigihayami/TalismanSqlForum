@{
    ViewBag.Title = "Пользователи";
    Layout = "~/Views/Shared/_LayoutAdmi.cshtml";
}
<div class="panel">
    <div class="panel-header bg-lightBlue fg-white">
        <h3>
            <i class="icon-user"></i>
            Пользователи
        </h3>
    </div>
    <div class="panel-content">
        <div class="listview">
            @foreach (var item in (IEnumerable<TalismanSqlForum.Models.ApplicationUser>)ViewData["_users"])
            {
                <div class="list-content">
                    <a href="@Url.Action("UsersGet", "Admin", new {UserId = item.UserName })" class="list">
                        <h4>@item.UserName</h4>
                    </a>
                </div>
            }
        </div>
    </div>
</div>

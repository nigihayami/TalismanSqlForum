<div class="panel" data-role="panel">
    <div class="panel-header bg-darkGreen fg-white">
        Модераторы
    </div>
    <div class="panel-content">
        <div class="row">
            <table class="table striped bordered hovered">
                <thead>
                    <tr>
                        <th class="text-left">Email</th>
                        <th class="text-left">Учреждение</th>
                        <th class="text-left">Телефон</th>
                        <th class="text-left">ИНН</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        var t = (List<TalismanSqlForum.Models.ApplicationUser>)ViewData["users_moderator"];
                        foreach (var item in t.OrderByDescending(a => a.DateReg))
                        {
                            <tr>
                                <td>@item.Email</td>
                                <td>@item.Mnemo_Org</td>
                                <td>@item.PhoneNumber</td>
                                <td>@item.Inn</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
